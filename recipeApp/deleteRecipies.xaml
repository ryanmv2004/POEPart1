<Window x:Class="recipeApp.deleteRecipies"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:recipeApp"
        mc:Ignorable="d"
        Title="deleteRecipies" Height="450" Width="800">
    <Grid Background="#FFA1E9D2">
        <Button x:Name="delBtn" Content="Yes" HorizontalAlignment="Left" Margin="325,170,0,0" VerticalAlignment="Top" Click="delBtn_Click"/>
        <Button x:Name="goBackBtn" Content="Go Back" HorizontalAlignment="Left" Margin="438,170,0,0" VerticalAlignment="Top" Click="goBackBtn_Click"/>
        <Label Content="Do You Want to Erase All Recipies?" HorizontalAlignment="Center" Margin="0,65,0,0" VerticalAlignment="Top" FontSize="18"/>

    </Grid>
</Window>
